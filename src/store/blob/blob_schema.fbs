namespace blobstore;

// Table for storing a BLOB.
table Blob {
  id:int64; // Unique identifier for the BLOB.
  data:[ubyte]; // The binary data of the BLOB. 'ubyte' is used for binary data.
  metadata:string; // Optional metadata.
}

// Root type can be set if you plan to store individual BLOB records directly.
root_type Blob;
